   "source": [
    "def print_board(board):\n",
    "    for row in board:\n",
    "\n",
    "        print('|'.join(row))\n",
    "        print('_' * 5)\n",
    "board = [[' ' for _ in range(3)] for _ in range(3)]\n",
    "\n",
    "print_board(board)\n"
   ]
