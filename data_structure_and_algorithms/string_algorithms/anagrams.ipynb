    "#  O(n log n)\n",
    "def is_anagram(s1, s2):\n",
    "    s1 = s1.replace(' ','').lower()\n",
    "    s2 = s2.replace(' ','').lower()\n",
    "    if sorted(s1) == sorted(s2):\n",
    "        return True\n",
    "    else:\n",
    "        return False\n",
    "s1 = 'Emperor Octavian'\n",
    "s2 = 'Captain over Rome'\n",
    "print(is_anagram(s1,s2))"
