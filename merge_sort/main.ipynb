   "source": [
    "def merge_sort(array):\n",
    "    middle_point = len(array) // 2\n",
    "\n",
    "    left_part = array[:middle_point]\n",
    "    right_part = array[middle_point:]\n",
    "    merge_sort(left_part)\n",
    "    merge_sort(right_part)"
   ]
  }
