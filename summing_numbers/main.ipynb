   ],
   "source": [
    "def mysum(number_list, start = 0):\n",
    "    output = start\n",
    "    for number in number_list:\n",
    "        output += number\n",
    "    return output\n",
    "print(mysum([30, 56, 45, 100], 90))\n",
    "print(mysum([90,78], 0))"
   ]
